---
import TerminalIcon from '../../atoms/terminal-icon/TerminalIcon.astro';
import Terminal from '../terminal/Terminal.astro';
import './skills-on-terminal.css';
---

<Terminal nombre="RafaeDev">
	<div class="terminal__skills">
		<h2>Mis habilidades</h2>
		<div class="terminal__skills__container">
			<TerminalIcon
				src="/assets/mysql.svg"
				color="blue"
				name="MYSQL"
			/>
			<TerminalIcon
				src="/assets/angular.svg"
				color="#FF4747"
				name="Angular"
			/>
			<TerminalIcon src="/assets/vue.svg" color="#1bc43f" name="Vue" />
			
			<TerminalIcon
				src="/assets/react.svg"
				color="#00D1FF"
				name="React"
			/>
			
		
			<TerminalIcon
				src="/assets/Astro.svg"
				color="red"
				name="Astro"
			/>
			<TerminalIcon
				src="/assets/mongodb.svg"
				color="green"
				name="MYSQL"
			/>
			<TerminalIcon
				src="/assets/nodejs.svg"
				color="green"
				name="Node js"
			/>
		</div>
	</div>
</Terminal>
